{
    "name": "vscode-better-netlinx",
    "displayName": "vscode-better-netlinx",
    "description": "Better syntax highlighting for NetLinx",
    "version": "0.0.1",
    "engines": {
        "vscode": "^1.98.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "languages": [
            {
                "id": "netlinx",
                "aliases": [
                    "NetLinx",
                    "netlinx"
                ],
                "extensions": [
                    ".axs",
                    ".axi"
                ],
                "configuration": "./language-configuration.json"
            }
        ],
        "grammars": [
            {
                "language": "netlinx",
                "scopeName": "source.netlinx",
                "path": "./syntaxes/netlinx.tmLanguage.json"
            }
        ]
    }
}
